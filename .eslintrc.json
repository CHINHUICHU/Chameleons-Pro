{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
        // "jQuery": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-restricted-syntax": ["error", "FunctionExpression", "WithStatement", "BinaryExpression[operator='in']"],
        "operator-linebreak": ["error", "after"],
        "comma-dangle": ["error", {
            "arrays": "never",
            "objects": "only-multiline",
            "imports": "never",
            "exports": "never",
            "functions": "never"
        }]
    }
}
